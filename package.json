{
    "name": "safa-uix",
    "version": "0.1.0",
    "description": "npm package for add reusable layer to quasar for safa company.",
    "main": "dist/safa-uix.js",
    "engines": {
        "node": ">= 7.0"
    },
    "scripts": {
        "dev": "babel-node build/local",
        "build": "rm -rf dist/ && babel-node build/lib",
        "docs": "babel-node build/docs",
        "lint": "eslint --ext .js,.vue docs src build test",
        "test": "jest",
        "coverage": "yarn test --coverage",
        "githooks": "sh build/git-hooks/install.sh",
        "changelog": "babel-node build/generate-changelog.js",
        "release": "sh build/release.sh",
        "new-component": "babel-node build/new-component.js",
        "github-release": "github-release",
        "commit": "git-cz"
    },
    "dependencies": {},
    "devDependencies": {
        "quasar": "^1.0.5"
    },
    "peerDependencies": {},
    "optionalDependencies": {},
    "files": [
        "dist",
        "doc",
        "lang",
        "lib",
        "src",
        "test"
    ],
    "keywords": [
        "safa",
        "framework",
        "vuejs",
        "vue",
        "quasar",
        "js",
        "phone",
        "tablet",
        "desktop",
        "spa",
        "pwa",
        "website",
        "electron"
    ],
    "author": {
        "name": "nazemi-safa",
        "email": "nazemi.works@gmail.com"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/penkong/safa-uix/issues"
    },
    "homepage": "https://github.com/penkong/safa-uix#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/penkong/safa-uix.git"
    }
}